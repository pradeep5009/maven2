pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload_Loans')
        {
            steps
            {
                git 'https://github.com/pradeep5009/maven2.git'
            }
        }
        stage('ContinuousBuild_Loans')
        {
            steps
            {
                sh 'mvn package'
            }
        }
       
          
     }         
}
